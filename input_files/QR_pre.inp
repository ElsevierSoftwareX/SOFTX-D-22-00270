stdout_proc = 0

! DEIM interpolant format
! 0: [P^T * URes]^+ (Full residual)
! 1: USol^T * URHS * [P^T * URHS]^+ (Galerkin format)
! 2: [P^T * URHS]^+, [P^T * USol]^+, and USol^T * URHS (separate residual bases)
regressorFormat = 0

!!!!! Residual/RHS Basis !!!!!
resInputString = input|binaryset|/home/chris/Research/GEMS_runs/straightDuct_3D/dataProc/podBases/0p15ms_to_0p16ms_samp1/Spatial_Modes/Spatial_Mode_|.bin|1|25|1

!!!!! Solution Basis !!!!!
solInputString = input|binaryset|/home/chris/Research/GEMS_runs/straightDuct_3D/dataProc/podBases/0p15ms_to_0p16ms_samp1/Spatial_Modes/Spatial_Mode_|.bin|1|25|1

!!!!! System Data !!!!!
nCells = 160000
nVars = 9

!!!!! Sampling Inputs !!!!!

! 0 = no oversampling
! 1 = random
! 2 = eigenvalue
! 3 = GNAT (Peherstorfer)
! 4 = GNAT (Carlberg)
sampType = 1

! For greedy sampling (sampType > 1)
! 0 = sample by DOF
! 1 = sample by cell aggregate
sampMethod = 1

! boundary sampling
numSampledBounds = 0
boundLabel1 = 1
boundPerc1  = 0.25
boundLabel2 = 2
boundPerc2  = 0.5

! itype binary file generated by pgrid_deim
dfd_itype_file = /home/chris/Research/GEMS_runs/straightDuct_3D/mesh/partition_10_itype/dfd_itype.bin

! percent of points to be sampled (If points is less than boundary cells will cause issues)
pSampling = 0.01

