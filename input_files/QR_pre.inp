stdout_proc = 0

! DEIM interpolant format
! 0: [P^T * URHS]^+ (MP-LSVT)
! 1: USol^T * URHS * [P^T * U]^+ (Galerkin format)
interpolantFormat = 0

!!!!! RHS Basis !!!!!
readRHSSnaps = false
rhsInputString = input|binaryset|/home/chris/Research/GEMS_runs/straightDuct_3D/dataProc/podBases/0p15ms_to_0p16ms_samp1/Spatial_Modes/Spatial_Mode_|.bin|1|25|1

! not required if loading from binary modes
numModesRHS = 25 

!!!!! Solution Basis !!!!!
readSolSnaps = false
solInputString = input|binaryset|/home/chris/Research/GEMS_runs/straightDuct_3D/dataProc/podBases/0p15ms_to_0p16ms_samp1/Spatial_Modes/Spatial_Mode_|.bin|1|25|1

! not required if loading from binary modes
numModesSol = 25

!!!!! System Data !!!!!
nCells = 160000
nVars = 9

!!!!! Sampling Inputs !!!!!

! 0 = no oversampling
! 1 = random
! 2 = eigenvalue
! 3 = DEIM greedy (not implemented)
sampType = 1

! For greedy sampling (sampType > 1)
! 0 = sample by DOF
! 1 = sample by cell aggregate
sampMethod = 1

! boundary sampling
numSampledBounds = 0
boundLabel1 = 1
boundPerc1  = 0.25
boundLabel2 = 2
boundPerc2  = 0.5

! itype binary file generated by pgrid_deim
dfd_itype_file = /home/chris/Research/GEMS_runs/straightDuct_3D/mesh/partition_10_itype/dfd_itype.bin

! percent of points to be sampled (If points is less than boundary cells will cause issues)
pSampling = 0.01

! Output options
deimModesPrefix = pinv_mode_

